from turtle import *
screensize(1000, 1000)
tracer(10)
n = 10
left(90)
down()
for i in range(9):
    forward(12 * n)
    right(90)
    forward(6 * n)
    right(90)
up()
forward(1 * n)
right(90)
forward(3 * n)
left(90)
down()
for i in range(9):
    forward(53 * n)
    right(90)
    forward(75 * n)
    right(90)
tracer(0)
up()
for x in range(-100, 100):
    for y in range(-100, 100):
        goto(x * n, y * n)
        dot(5, 'red')
done()
